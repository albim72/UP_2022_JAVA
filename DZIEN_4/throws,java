public class Main {

    static void nowametoda() throws IllegalAccessException
    {
        System.out.println("to jest wnÄ™trze metody nowametoda()");
        throw new IllegalAccessException("pokaz");
    }
    public static void main(String[] args) {

        try{
            nowametoda();
        }catch (IllegalAccessException e){
            System.out.println("przechwycono w metodzie main!!!");
        }
    }
}
